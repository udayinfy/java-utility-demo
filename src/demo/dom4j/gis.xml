	<gml:featureMember>
		<!-- 第一条记录 开始 -->
		<esri:L416 fid="L416.1">
			<!-- esri是命名空间，没有意义，L416是图层名，.1表示是这个图层的第一条记录 -->
			<esri:OBJECTID>15</esri:OBJECTID>
			<!-- Objectid是主键字段,15是主键值 -->
			<gml:multiPointProperty>
				<!-- gml也是命名空间，multiPointProperty表示开始定义图形 -->
				<gml:MultiPoint srsName="EPSG:4326">
					<!-- MultPoint表示图形类型，点图层都是这样，srsName表示坐标系代号 -->
